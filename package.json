{
  "name": "@y/hub",
  "version": "0.2.1",
  "description": "Scalable websocket provider for Yjs",
  "sideEffects": false,
  "type": "module",
  "funding": {
    "type": "GitHub Sponsors ‚ù§",
    "url": "https://github.com/sponsors/dmonad"
  },
  "scripts": {
    "start": "docker compose --profile all up",
    "start:dbs": "docker compose --profile db up",
    "docker:rebuild": "docker compose --profile all build --no-cache",
    "debug:server": "node --env-file .env --inspect-brk ./bin/server.js",
    "debug:worker": "node --env-file .env --inspect-brk ./bin/worker.js",
    "start:init": "node --env-file .env $(test -f .env.testing && echo '--env-file .env.testing') ./bin/init-db.js",
    "dist": "npm run types",
    "types": "tsc --skipLibCheck",
    "lint": "standard && tsc --skipLibCheck",
    "test": "node --expose-gc --max-old-space-size=8192 --env-file .env $(test -f .env.testing && echo '--env-file .env.testing') tests/index.js",
    "debug:test": "node --expose-gc --max-old-space-size=8192 --env-file .env $(test -f .env.testing && echo '--env-file .env.testing') --inspect-brk tests/index.js",
    "preversion": "npm run lint && npm run dist",
    "clean": "rm -rf **/dist "
  },
  "bin": {
    "yhub": "./bin/yhub.js"
  },
  "files": [
    "dist/*",
    "bin/*",
    "src/*"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "default": "./src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./plugins/s3": {
      "default": "./src/plugins/s3.js",
      "types": "./dist/src/storage/s3.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yjs/yhub.git"
  },
  "keywords": [
    "Yjs"
  ],
  "author": "Kevin Jahns <kevin.jahns@protonmail.com>",
  "license": "AGPL-3.0 OR PROPRIETARY",
  "bugs": {
    "url": "https://github.com/yjs/yhub/issues"
  },
  "homepage": "https://github.com/yjs/yhub#readme",
  "standard": {
    "ignore": [
      "/dist",
      "/node_modules",
      "/demos"
    ]
  },
  "dependencies": {
    "@y/protocols": "^1.0.6-3",
    "@y/y": "^14.0.0-22",
    "lib0": "^1.0.0-rc.1",
    "minio": "^8.0.6",
    "postgres": "^3.4.3",
    "redis": "^4.6.12",
    "uws": "github:uNetworking/uWebSockets.js#v20.57.0"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.6",
    "@types/ws": "^8.5.10",
    "@y/websocket": "^4.0.0-3",
    "concurrently": "^8.2.2",
    "standard": "^17.1.0",
    "standardjs": "^1.0.0-alpha",
    "typescript": "^5.9.3",
    "ws": "^8.16.0",
    "y-websocket": "^2.0.4"
  }
}
