{
  "name": "@y/hub",
  "version": "0.0.0",
  "description": "Scalable websocket provider for Yjs",
  "sideEffects": false,
  "type": "module",
  "funding": {
    "type": "GitHub Sponsors ‚ù§",
    "url": "https://github.com/sponsors/dmonad"
  },
  "scripts": {
    "dev": "concurrently -r 'npm run redis' 'npm run postgres' 'sleep 10 && npm run init && npm start'",
    "start": "concurrently -r 'npm run start:server' 'npm run start:worker'",
    "start:server": "node --env-file .env ./bin/server.js",
    "debug:server": "node --env-file .env --inspect-brk ./bin/worker.js ./bin/server.js",
    "start:worker": "node --env-file .env ./bin/worker.js",
    "debug:worker": "node --env-file .env --inspect-brk ./bin/worker.js",
    "redis": "docker run -p 6379:6379 redis",
    "postgres": "docker run -p 5432:5432 -e POSTGRES_USER=yredis -e POSTGRES_PASSWORD=yredis -e POSTGRES_DB=yredis postgres:16-alpine",
    "init": "node --env-file .env $(test -f .env.testing && echo '--env-file .env.testing') ./bin/init-db.js",
    "dist": "tsc",
    "lint": "standard && tsc",
    "test": "node --env-file .env $(test -f .env.testing && echo '--env-file .env.testing') tests/index.js",
    "debug:test": "node --env-file .env $(test -f .env.testing && echo '--env-file .env.testing') --inspect-brk tests/index.js",
    "preversion": "npm run lint && npm run dist",
    "clean": "rm -rf **/dist "
  },
  "bin": {
    "y-redis-server": "./bin/server.js",
    "y-redis-worker": "./bin/worker.js"
  },
  "files": [
    "dist/*",
    "bin/*",
    "src/*"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "default": "./src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./storage/memory": {
      "default": "./src/storage/memory.js",
      "types": "./dist/src/storage/memory.d.ts"
    },
    "./storage/s3": {
      "default": "./src/storage/s3.js",
      "types": "./dist/src/storage/s3.d.ts"
    },
    "./storage/postgres": {
      "default": "./src/storage/postgres.js",
      "types": "./dist/src/storage/postgres.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yjs/yhub.git"
  },
  "keywords": [
    "Yjs"
  ],
  "author": "Kevin Jahns <kevin.jahns@protonmail.com>",
  "license": "AGPL-3.0 OR PROPRIETARY",
  "bugs": {
    "url": "https://github.com/yjs/yhub/issues"
  },
  "homepage": "https://github.com/yjs/yhub#readme",
  "standard": {
    "ignore": [
      "/dist",
      "/node_modules",
      "/demos"
    ]
  },
  "dependencies": {
    "@y/protocols": "^1.0.6-3",
    "@y/y": "^14.0.0-20",
    "lib0": "^1.0.0-0",
    "redis": "^4.6.12",
    "uws": "github:uNetworking/uWebSockets.js#v20.56.0"
  },
  "optionalDependencies": {
    "postgres": "^3.4.3"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.6",
    "@types/ws": "^8.5.10",
    "concurrently": "^8.2.2",
    "standard": "^17.1.0",
    "typescript": "^5.9.3",
    "ws": "^8.16.0",
    "y-websocket": "^2.0.4"
  }
}
